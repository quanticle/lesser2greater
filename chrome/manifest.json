{
    "name": "Lesser2Greater",
    "version": "2.0",
    "description": "Rewrite Less[er]Wrong links to point to GreaterWrong",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "*://*.lesswrong.com/*",
        "*://*.greaterwrong.com/*",
        "webNavigation", 
        "tabs"]
}
